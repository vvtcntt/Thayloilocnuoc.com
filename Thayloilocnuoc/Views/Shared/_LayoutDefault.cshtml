    <script>
        window.fbAsyncInit = function () {
            FB.init({
                appId: '1671852036431510',
                xfbml: true,
                version: 'v2.5'
            });
        };
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
