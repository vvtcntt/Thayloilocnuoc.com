            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>


</head>
<body>
    <header>
        @{Html.RenderAction("BannerPatial", "Header");}
        <div class="line"></div>
    </header>
    <section>
        @RenderBody()
  

    </section>
  @{Html.RenderAction("FooterPartial", "Footer");}
    <div id="fb-root"></div>
    <script type="text/javascript">

					function getLocation(id)
					{   $(".listLocation li a").removeClass("set");
						 $(".contentLocation").removeClass("showLocation");
						$("#"+id).addClass("showLocation");
						$("."+id).addClass("set");
 					}



    </script>
    <script>
(function (d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v2.5&appId=1671852036431510";
    fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
</body>
</html>
